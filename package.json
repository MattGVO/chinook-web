{
  "name": "chinook-web",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.js",
  "scripts": {
    "start": "npm-run-all -p serve watch",
    "serve": "http-server",
    "watch": "watchify src/index.js -o bundle.js --debug",
    "build": "browserify src/index.js | uglifyjs > bundle.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-env": "^1.2.2",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "browserify-css": "^0.10.0",
    "http-server": "^0.9.0",
    "npm-run-all": "^4.0.2",
    "uglify-js": "^2.8.16",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "codemirror": "^5.25.0",
    "normalize.css": "^5.0.0",
    "preact": "^7.2.1",
    "sql.js": "^0.4.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "env",
              {
                "targets": {
                  "browsers": [
                    "last 2 versions"
                  ],
                  "uglify": true
                },
                "useBuiltins": true
              }
            ]
          ]
        }
      ],
      "browserify-css"
    ]
  }
}
